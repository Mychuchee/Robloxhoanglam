local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "MEmaybeo Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "Skibidi"})

local PlayerTab = Window:MakeTab({
	Name = "Player",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})



-- Assuming you have Orion Library integrated
local Orion = require(game.ReplicatedStorage.Orion)


-- Add a Checkbox Toggle to the PlayerTab
local slowAttackToggle = playerTab:Checkbox("Slow Attack", function(value)
    _G.FastAttack = value -- Update your global variable

    if value then
        Attack1Function()
    end

    if _G.FastAttack then
        -- Execute your existing code
        local xShadowFastAttackx = require(game:GetService("Players").LocalPlayer.PlayerScripts.CombatFramework)
        local xShadowx = debug.getupvalues(xShadowFastAttackx)[2]

        if typeof(xShadowx) == "table" then
            pcall(function()
                xShadowx.activeController.timeToNextAttack = (math.huge^math.huge^math.huge)
                xShadowx.activeController.timeToNextAttack = 0
                xShadowx.activeController.hitboxMagnitude = 200
                xShadowx.activeController.active = false
                xShadowx.activeController.timeToNextBlock = 0
                xShadowx.activeController.focusStart = 0
                xShadowx.activeController.increment = 4
                xShadowx.activeController.blocking = false
                xShadowx.activeController.attacking = false
                xShadowx.activeController.humanoid.AutoRotate = 50
            end)
        end
    end
end)

-- Optionally, set default state of the toggle
slowAttackToggle:SetState(false)  -- Set to true if you want it to be initially checked



OrionLib:Init()
