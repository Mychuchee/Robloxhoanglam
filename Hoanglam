local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "X2Paluw Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local PlayerTab = Window:MakeTab({
    Name = "Tab 1",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false

PlayerTab:AddToggle({
    Name = "Speed!",
    Default = false,
    Callback = function(Value)
    game.Players.LocalPlayer.Character:SetAttribute("SpeedMultiplier", 4)
    end
})

PlayerTab:AddToggle({
    Name = "Jump!",
    Default = false,
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 300
    end
})

OrionLib:Init()
